{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "hane-73789",
          "appId": "1:15841456633:android:e4e95ede0eadb93c87cd28",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "hane-73789",
          "appId": "1:15841456633:ios:579519159159161487cd28",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "hane-73789",
          "configurations": {
            "android": "1:15841456633:android:e4e95ede0eadb93c87cd28",
            "ios": "1:15841456633:ios:579519159159161487cd28",
            "web": "1:15841456633:web:e926de8134a16a8d87cd28"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(html|js|css|json|wasm|png|jpg|jpeg|svg|ico|woff|woff2|ttf|eot)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          },
          {
            "key": "Expires",
            "value": "0"
          }
        ]
      },
      {
        "source": "/service-worker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}